{
  "workbench.startupEditor": "none",
  "svelte.enable-ts-plugin": true,
  "workbench.colorTheme": "Ayu Dark",
  "editor.fontSize": 16,
  "explorer.confirmDelete": false,
  "debug.console.fontSize": 16,
  "scm.inputFontSize": 16,
  "terminal.integrated.fontSize": 16,
  "chat.editor.fontSize": 16,
  "markdown.preview.fontSize": 16,
  "editor.inlineSuggest.suppressSuggestions": true,
  "editor.formatOnSave": true,
  "editor.foldingImportsByDefault": true,
  "editor.showFoldingControls": "always",
  "editor.minimap.enabled": false,
  "editor.fontFamily": "'Hack', Hack",
  "git.confirmSync": false,
  "git.autofetch": true,
  "workbench.colorCustomizations": {
    "sideBar.foreground": "#ffffff",
    "list.inactiveSelectionForeground": "#e28000",
    "list.activeSelectionForeground": "#e28000"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "comment.block.documentation",
          "comment.block.documentation.js",
          "comment.line.double-slash.js",
          "storage.type.class.jsdoc",
          "entity.name.type.instance.jsdoc",
          "variable.other.jsdoc",
          "punctuation.definition.comment",
          "punctuation.definition.comment.begin.documentation",
          "punctuation.definition.comment.end.documentation"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#474747"
        }
      }
    ]
  },
  "editor.smoothScrolling": false,
  "git.enableSmartCommit": true,
  "gopls": {
    "ui.semanticTokens": true
  },
  "workbench.reduceMotion": "on",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "go.survey.prompt": false,
  "go.showWelcome": false,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "always",
    "source.fixAll.eslint": "always"
  },
  "editor.tabSize": 2,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    "*.env": "properties"
  },
  "explorer.confirmDragAndDrop": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "redhat.telemetry.enabled": false,
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "telemetry.telemetryLevel": "off",
  "diffEditor.ignoreTrimWhitespace": false,
  "git.openRepositoryInParentFolders": "never",
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false
  },
  "go.toolsManagement.autoUpdate": true,
  "github.copilot.editor.enableAutoCompletions": true,
  "playwright.showTrace": false,
  "githubIssues.issueBranchTitle": "zergo0/Dev-${issueNumber}",
  "makefile.configureOnOpen": false,
  "playwright.reuseBrowser": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "go.lintTool": "golangci-lint",
  "go.lintFlags": ["--fast"],
  "git.ignoreRebaseWarning": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "cursor.cpp.disabledLanguages": ["scminput"],
  "cursor.composer.collapsePaneInputBoxPills": true,
  "comments.openView": "never",
  "workbench.editor.wrapTabs": true,
  "githubPullRequests.commentExpandState": "collapseAll",
  "window.commandCenter": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "json-to-go.io.input": "clipboard",
  "go.inlayHints.assignVariableTypes": true,
  "go.inlayHints.compositeLiteralFields": true,
  "go.inlayHints.compositeLiteralTypes": true,
  "go.inlayHints.constantValues": true,
  "go.inlayHints.rangeVariableTypes": true,
  "go.inlayHints.functionTypeParameters": true,
  "go.inlayHints.parameterNames": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "roo-cline.allowedCommands": [
    "npm test",
    "npm install",
    "tsc",
    "git log",
    "git diff",
    "git show",
    "bun run check",
    "bun run format",
    "bun run lint",
    "ls",
    "find",
    "curl",
    "date",
    "cp",
    "mv",
    "mkdir",
    "touch",
    "cd",
    "go fmt"
  ],
  "terminal.integrated.defaultProfile.osx": "zsh",
  "tailwind-fold.foldStyle": "QUOTES",
  "tailwind-fold.autoFold": false,
  "update.releaseTrack": "prerelease",
  "githubPullRequests.pullBranch": "never",
  "eslint.probe": [
    "astro",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "svelte"
  ],
  "eslint.validate": [
    "astro",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "svelte"
  ],
  "augment.disableFocusOnAugmentPanel": true,
  "amp.notifications.enabled": false,
  "amp.tab.enabled": true,
  "typescript.tsserver.nodePath": "/opt/homebrew/bin/node",
  "typescript.tsserver.maxTsServerMemory": 8192,
  "extensions.ignoreRecommendations": true,
  "amp.tab.hoverDiagnosticSuggestions": true
}
